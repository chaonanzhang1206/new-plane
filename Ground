import java.awt.image.BufferedImage;
import javax.imageio.ImageIO;

public class Ground {

BufferedImage image;

int x,y;

int width;

int heigth;

public Ground() throws Exception{

image = ImageIO.read(getClass().getResource("ground.png"));

width = image.getWidth();

heigth = image.getHeight();

x = 0;

y = 500;

}

public void step(){

x--;

if ( x == -109 ) {

x=0;

}

}



}
